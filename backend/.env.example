# Copy this file to .env and fill values before running the backend
# MongoDB connection string (local or Atlas)
MONGODB_URI=mongodb://localhost:27017/focus-proctoring-db

# Cloudinary credentials (only required for uploads)
CLOUD_NAME=your_cloudinary_cloud_name
API_KEY=your_cloudinary_api_key
API_SECRET=your_cloudinary_api_secret

# Optional: port override (default in app is 3000)
# PORT=3000
